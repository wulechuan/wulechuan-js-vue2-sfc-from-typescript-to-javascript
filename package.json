{
    "name": "@wulechuan/vue2-sfc-from-typescript-to-javascript",
    "version": "0.1.9",
    "author": "wulechuan <wulechuan@live.com>",
    "license": "WTFPL",
    "description": "This tool treats a string as content of a `.vue` file, with its `<script>` part written in TypeScript, and covert the entire file content string into a new string, as the content of a new `.vue` file, where its `<script>` block is written in JavaScript.",
    "main": "./source/index.js",
    "scripts": {
        "remove-all-html-docs": "rimraf  ./ReadMe.html  ./documents/**/*.html",
        "generate-html-docs": "npm  run  remove-all-html-docs    &&    wlc-md-to-html  -i ./*.md,./documents/**/*.md  -E3  -C ./documents/wlc-md-to-html.config.js  --to '*'",
        "lint": "eslint  --fix  ./source/**/*.js  ./tests/**/*.js",
        "test": "npx  babel-node  tests/index.js",
        "start": "npm  test",
        "before-git-commit": "npm  i                &&    npm  start",
        "prepublishOnly": "rimraf  ./ReadMe.html    &&    npm  start"
    },
    "keywords": [
        "vue",
        "sfc",
        "single-file-component",
        "single-file-components",
        "single-filed-components",
        "single-filed-components",
        "vue-sfc",
        "vue-single-file-component",
        "vue-single-file-components",
        "vue-single-filed-component",
        "vue-single-filed-components",
        "parser",
        "sfc-parser",
        "vue-sfc-parser",
        "vue2",
        "javascript",
        "js",
        "es",
        "ecmascript",
        "typescript",
        "ts",
        "transpile",
        "compile",
        "convert",
        "单文件组件",
        "vue单文件组件",
        "代码转译",
        "代码转换"
    ],
    "homepage": "https://github.com/wulechuan/wulechuan-js-vue2-sfc-from-typescript-to-javascript.git#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wulechuan/wulechuan-js-vue2-sfc-from-typescript-to-javascript.git"
    },
    "bugs": {
        "url": "https://github.com/wulechuan/wulechuan-js-vue2-sfc-from-typescript-to-javascript.git/issues"
    },
    "dependencies": {
        "@wulechuan/get-valid-indentation-string": "^1.0.9",
        "@wulechuan/vue2-official-sfc-parser": "^0.1.8",
        "chalk": "^4.1.0",
        "hash-sum": "^2.0.0",
        "indent": "0.0.2",
        "indent.js": "^0.3.5",
        "less": "^3.12.2",
        "sass": "^1.26.10",
        "stylus": "^0.54.8"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/node": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@types/node": "^14.0.23",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@wulechuan/css-stylus-markdown-themes": "^6.4.12",
        "eslint": "^7.4.0",
        "fs-extra": "^9.0.1",
        "rimraf": "^3.0.2",
        "typescript": "^3.9.7",
        "vue": "^2.6.11",
        "vue-property-decorator": "^9.0.0"
    }
}
